{% extends "base.jinja2" %}

{% block content %}
<h1>Registration</h1>
<form id="registration-form">
  <input type="text" name="user">
  <input type="email" name="email">
  <input type="password" name="password">
  <input type="submit" name="submit" value="Sign-up">
</form>

<h1>Sign-in</h1>
<form id="signin-form">
  <input type="text" name="user">
  <input type="password" name="password">
  <input type="submit" name="submit" value="Sign-in">
</form>
{% endblock content %}
